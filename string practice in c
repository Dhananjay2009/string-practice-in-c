#include <stdio.h>
#include <string.h>

int main()
{
    char str1[34];
    char str2[24];
    char str3[29] = " is a friend of ";

    printf("Enter name of first friend\n");
    gets(str1);
    printf("Enter name of second friend\n");
    gets(str2);

    printf(strcat(str1,(strcat (str3,str2))));

    return 0;
}
  
